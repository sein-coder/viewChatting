<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="member" >
	<insert id="insertMember" parameterType="com.kh.spring.member.model.vo.Member">
		INSERT INTO MEMBER VALUES(#{userId},#{password},#{userName},
					#{gender},#{age},#{email},#{phone},
					#{address},#{hobby, typeHandler=strArray},default)
	</insert>
	<select id="selectMemberOne" parameterType="com.kh.spring.member.model.vo.Member"
	resultType="com.kh.spring.member.model.vo.Member">
		SELECT * FROM MEMBER WHERE USERID=#{userId}
	</select>
	
	
</mapper>





